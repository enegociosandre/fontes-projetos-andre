SetPrvt("CDESC2		, CDESC3	, CSTRING	, NTIPO		, NOMEPROG"				)
SetPrvt("NTOTREGS	, NMULT		, NPOSANT	, NPOSATU	, NPOSCNT	, LCONTINUA")
