If Len(aEntradas)>0
	nTot:=Len(aEntradas)
	For i:=1 to Len(aEntradas)
		cCFO:=aEntradas[i,1]
		If !(Substr(cCFO,2,2)=="00")
			nPos:=Ascan(aEntradas,{|x|x[1]==Substr(cCfo,1,1)+"00"})
