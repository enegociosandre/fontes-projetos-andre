        If IsDigit(Substr(cCampo,1,1)).and.IsDigit(Substr(cCampo,3,1))
            cValor:=Substr(cLinha,52,18)
            cValor:=StrTran(cValor,'.')
            cValor:=StrTran(cValor,',','.')
            nValor:=Val(cValor)
            cTexto:=Substr(cLinha,5,50)
		Else
            nValor:=0
            cTexto:=Substr(cLinha,5)
        Endif
        Do Case 
            Case cCampo=='001'
                n001:=n001+nValor
			Case cCampo=='002'
                n002:=n002+nValor
			Case cCampo=='003'
                n003:=n003+nValor
