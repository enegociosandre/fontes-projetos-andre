Static Function GravaCtaCtb()

	Local aArr		
	Local oTecCtb 	:= TecxCtb():New()
&&	Local cConta	:= oTecCtb:RetCliForCta("SA1", "A1_ZZITEM")


&&	SA1->(RecLock("SA1", .F.))
&&		SA1->A1_ZZITEM 	:= cConta
&&	SA1->(MsUnlock())

	&& Criacao das Contas
&&	oTecCtb:CriaCtaCtb({cConta,cAdiant,cApropr}, "SA1")
	
	
	&& Atualiza Cliente
	RecLock("SA1",.f.)
		SA1->A1_ZZITEM	:= 	oTecCtb:RetCliForCta( "SA1" , "A1_ZZITEM"	)
	MsUnlock("SA1")
	
	&& Criacao do Item				  
	aArr	:= 	{ oTecCtb:RetCliForCta( "SA1" , "A1_ZZITEM"		) }
	
	oTecCtb:CriaCtaCtb( aArr , "SA1" )	

Return