User Function FA050INC()

Local _lRet 	:= .t.                                               
Local _xArea	:= GetArea()
                      
// Fausto 26/01/12
	IF (M->E2_PREFIXO == "MAN")
		IF (M->E2_TIPO == "OP ")
			SETMV("MV_ZZPROP",STRZERO(VAL(M->E2_NUM)+1,9))
		ENDIF
		IF (M->E2_TIPO == "RD ")
			SETMV("MV_ZZPRRD",STRZERO(VAL(M->E2_NUM)+1,9))
		ENDIF
		IF (M->E2_TIPO == "PA ")
			SETMV("MV_ZZPRPA",STRZERO(VAL(M->E2_NUM)+1,9))
		ENDIF
	EndIF
	
	IF (M->E2_PREFIXO == "GPE")
		IF (M->E2_TIPO == "FOL")
			SETMV("MV_ZZPRFOL",STRZERO(VAL(M->E2_NUM)+1,9))
		ENDIF
	EndIF
// Fim 26/01/12


&& Validação: Obrigao preenchimento do Centro de Custo quando nao utilizado o Rateio
If M->E2_RATEIO<>"S" .And. Empty(M->E2_CCD) .And. M->E2_PREFIXO<>"GPE"  // Fausto 26/01/12 If M->E2_RATEIO<>"S" .And. Empty(M->E2_CCD)
	MsgStop("Atenção: O centro de custo deve ser informado.")
	_lRet := .f.
EndIf
		
RestArea(_xArea)
Return _lRet