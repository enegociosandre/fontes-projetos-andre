#Include "RwMake.ch"
#Include 'Protheus.ch'
#Include 'Topconn.ch'

User Function ETQCB8()
	Local aArea := GetArea()
	Local cPerg := 'ETQCB8'
	
	ValidPerg(cPerg)
	
	If !(Pergunte(cPerg,.T.))
		Return
	Endif
	
	Processa( {|| PrintETQ() },"imprimindo Etiqueta de Expedição..." )  
	
Return
    

Static Function PrintETQ()
