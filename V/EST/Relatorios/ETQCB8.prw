	Local cQuery	:= ""
	Local cPorta   	:= "LPT1"
	Local nQtdEtq	:= 0
	Local nQuant	:= 0
	Local nResto	:= 0  
	Local nX		:= 0
	Local cCodBar	:= ""

	if Select(cAliasCB8) <> 0
		(cAliasCB8)->(dbCloseArea())
	endif
	
	cQuery := "SELECT * "
	cQuery += "FROM " + RetSQLName("CB8") + " CB8, "
	cQuery += "     " + RetSQLName("SB1") + " B1 "
	cQuery += "WHERE "
	cQuery += "  CB8_FILIAL  = '" + xFilial("CB8") + "' AND "
	cQuery += "  CB8_PROD = B1_COD AND "
	cQuery += "  CB8_PROD>= '" + mv_par01 + "' AND CB8_PROD <= '" + mv_par02 + "' AND "
	cQuery += "  CB8_ORDSEP  = '" + mv_par03 + "' AND "
//	cQuery += "  B1_QE <> 1 AND "						Habilitar somente esse filtro depois de etiquetarem todas os produtos.
	cQuery += "  CB8.D_E_L_E_T_ = ' '  AND "
	cQuery += "  B1.D_E_L_E_T_ = ' '   "
	cQuery += "ORDER BY CB8_ORDSEP+CB8_LOCAL+CB8_LCALIZ "
	
